<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.homepage.dao.HomePagePhotoMapper">



    <select id="selectHomePagePhoto" resultType="HomePagePhoto">
        SELECT id,likecount,illustration,address,bigaddress FROM homepage_photo
    </select>

    <insert id="insertUser" parameterType="User">
        INSERT INTO `user`(uemail,uname,upassword,usex,uage) VALUES(#{uemail},#{uname},#{upassword},#{usex},#{uage})
    </insert>










</mapper>
